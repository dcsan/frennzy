						</VisualStateManager.VisualStateGroups>
					</Button>
				</Grid>
				<Grid x:Name="pnl_ListenersVote" Visibility="Collapsed">
					<Grid x:Name="pnl_ListenersVote_S" HorizontalAlignment="Center" VerticalAlignment="Center" Width="320" Height="480">
						<TextBlock Style="{StaticResource BasicTextBlock-Sketch}" Text="Now we wait..." VerticalAlignment="Top" FontSize="48" Margin="0,8,0,0" d:LayoutOverrides="Width" HorizontalAlignment="Center"/>
						<TextBlock Style="{StaticResource BasicTextBlock-Sketch}" VerticalAlignment="Center" FontSize="32" Margin="10" TextWrapping="Wrap" Text="Tallying votes..." HorizontalAlignment="Center"/>
						<WebBrowser x:Name="web_MoreInfo" Margin="0,70,0,0" Source="http://www.google.com" Visibility="Collapsed"/>
					</Grid>
					<Grid x:Name="pnl_ListenesVote_L" HorizontalAlignment="Center" VerticalAlignment="Center" Width="320" Height="480" Visibility="Collapsed">
						<TextBlock HorizontalAlignment="Center" Style="{StaticResource BasicTextBlock-Sketch}" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="48" Text="Is that true?" Margin="0,8,0,0"/>
						<TextBlock Style="{StaticResource BasicTextBlock-Sketch}" TextWrapping="Wrap" FontSize="32" Text="{Binding VM.ChoiceText, Mode=OneWay}" d:LayoutOverrides="Width" Margin="10,99,10,140"/>
						<Button Style="{StaticResource ButtonStyle_iPhone}" VerticalAlignment="Bottom" Margin="5,0,5,5" Content="Nuh-Uh!!" Click="Button_GuessLie_Click">
							<Button.Background>
								<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
									<GradientStop Color="#FF6A0000" Offset="0.514"/>
									<GradientStop Color="#FFFFA6A6"/>
									<GradientStop Color="#FFCA1A1A" Offset="0.478"/>
									<GradientStop Color="#FF770303" Offset="1"/>
									<GradientStop Color="#FF911313" Offset="0.891"/>
								</LinearGradientBrush>
							</Button.Background>
							<VisualStateManager.CustomVisualStateManager>
								<ei:ExtendedVisualStateManager/>
							</VisualStateManager.CustomVisualStateManager>
							<VisualStateManager.VisualStateGroups>
								<VisualStateGroup x:Name="TruthStates1">
									<VisualState x:Name="True1"/>
									<VisualState x:Name="False1"/>
								</VisualStateGroup>
							</VisualStateManager.VisualStateGroups>
						</Button>
						<Button Style="{StaticResource ButtonStyle_iPhone}" VerticalAlignment="Bottom" Content="It's the Truth!" Margin="5,5,5,71" Click="Button_GuessTrue_Click">
							<VisualStateManager.CustomVisualStateManager>
								<ei:ExtendedVisualStateManager/>
							</VisualStateManager.CustomVisualStateManager>
							<VisualStateManager.VisualStateGroups>
								<VisualStateGroup x:Name="TruthStates2">
									<VisualState x:Name="True2"/>
									<VisualState x:Name="False2"/>
								</VisualStateGroup>
							</VisualStateManager.VisualStateGroups>
						</Button>
					</Grid>
				</Grid>
				<Grid x:Name="pnl_WaitForResults" Visibility="Collapsed">
					<TextBlock Style="{StaticResource BasicTextBlock-Sketch}" Text="Now we wait..." VerticalAlignment="Top" FontSize="48" Margin="0,8,0,0" HorizontalAlignment="Center"/>
					<TextBlock Style="{StaticResource BasicTextBlock-Sketch}" VerticalAlignment="Center" FontSize="32" Margin="10" TextWrapping="Wrap" Text="Tallying votes..." HorizontalAlignment="Center"/>
				</Grid>
				<Grid x:Name="pnl_Results" HorizontalAlignment="Center" VerticalAlignment="Center" Width="320" Height="480" Visibility="Collapsed">
					<Grid.RowDefinitions>
						<RowDefinition/>
						<RowDefinition Height="Auto"/>
					</Grid.RowDefinitions>
					<StackPanel Margin="8,8,7,0" Orientation="Horizontal" VerticalAlignment="Top" HorizontalAlignment="Center">
						<TextBlock Style="{StaticResource BasicTextBlock-Sketch}" Text="Round #" FontSize="48" HorizontalAlignment="Center"/>
						<TextBlock Style="{StaticResource BasicTextBlock-Sketch}" Text="{Binding VM.RoundNum}" FontSize="48" HorizontalAlignment="Center"/>
					</StackPanel>
					<Button x:Name="button1" Style="{StaticResource ButtonStyle_iPhone}" Content="Next Round!" Background="{StaticResource iPhoneButtonBlue}" Grid.Row="1" d:LayoutOverrides="Height">
						<i:Interaction.Triggers>
							<i:EventTrigger EventName="Click">
								<pi:ActivateStateAction TargetState="On"/>
								<ei:CallMethodAction MethodName="SetUpNextRound" TargetObject="{Binding VM}"/>
							</i:EventTrigger>
						</i:Interaction.Triggers>
						<VisualStateManager.CustomVisualStateManager>
							<ei:ExtendedVisualStateManager/>
						</VisualStateManager.CustomVisualStateManager>
						<VisualStateManager.VisualStateGroups>
							<VisualStateGroup x:Name="TruthStates3">
								<VisualState x:Name="True3"/>
								<VisualState x:Name="False3"/>
							</VisualStateGroup>
						</VisualStateManager.VisualStateGroups>
					</Button>
					<ItemsControl Margin="10,71,10,5" ItemsSource="{Binding VM.Players}" ItemTemplate="{StaticResource PlayerResultsDataTemplate}"/>
				</Grid>
			</Grid>
		</Button>
	</Grid>
</UserControl>